package-as: AzeriteUI
enable-nolib-creation: no

embedded-libraries:
    - ace3
    - ouf
    - libeditmodeoverride
    - taintless

optional-dependencies:
    - ace3
    - clique
    - taintless

tools-used:
    - ouf

move-folders:
    AzeriteUI/AzeriteUI: AzeriteUI

manual-changelog:
    filename: CHANGELOG.md
    markup-type: markdown

license-output: LICENSE.txt

ignore:
    - CHANGELOG.md
    - CODE_OF_CONDUCT.md
    - LICENSE.md
    - README.md
